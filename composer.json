{
    "name": "root/lottery",
    "type": "project",
    "authors": [
        {
            "name": "Alvi Isaknderov"
        }
    ],
    "description": "Lottery Service Project",
    "version": "1.0.0",
    "require": {
        "php": ">=8.2",
        "ext-pcntl": "*",
        "symfony/flex": "^2.4.3",
        "symfony/cache-contracts": "^3.4",
        "symfony/framework-bundle": "v7.0.2",
        "symfony/runtime": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/dependency-injection": "v7.0.2",
        "symfony/console": "v7.0.2",
        "symfony/http-kernel": "v7.0.2",
        "symfony/http-foundation": "^7.0",
        "symfony/routing": "v7.0.2",
        "symfony/config": "^7.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.17",
        "ramsey/uuid": "^4.7",
        "symfony/dotenv": "v7.0.2",
        "ramsey/uuid-doctrine": "^2.0",
        "kwn/php-rdkafka-stubs": "^2.2",
        "doctrine/common": "^3.4",
        "symfony/twig-bundle": "^7.0.0",
        "symfony/options-resolver": "^7.0.0",
        "symfony/asset": "^7.0.0",
        "twig/twig": "^3.7.1|^3.8.0",
        "twig/extra-bundle": "^3.7.1|^3.8.0",
        "symfony/validator": "v7.0.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.25.0",
        "symfony/property-access": "^7.0.0",
        "symfony/property-info": "^7.0.0",
        "symfony/serializer": "v7.0.2",
        "react/event-loop": "^1.5",
        "doctrine/annotations": "^2.0",
        "symfony/scheduler": "^7.0.0",
        "symfony/process": "v7.0.2",
        "symfony/messenger": "^7.0.0",
        "symfony/monolog-bundle": "^3.10",
        "react/child-process": "^0.6.5",
        "react/promise": "^3.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.8.1",
        "symfony/polyfill": "^1.28",
        "vimeo/psalm": "^5.19.0",
        "phpunit/phpunit": "10.5.5",
        "slevomat/coding-standard": "^8.14",
        "symfony/browser-kit": "^7.0.0",
        "symfony/css-selector": "^7.0.0",
        "symfony/phpunit-bridge": "v7.0.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0.0"
        }
    }
}
