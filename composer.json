{
    "name": "root/lottery",
    "type": "project",
    "authors": [
        {
            "name": "Alvi"
        }
    ],
    "require": {
        "symfony/flex": "^2.4",
        "symfony/cache-contracts": "^3.4",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/console": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/routing": "^7.0",
        "doctrine/annotations": "^2.0",
        "symfony/config": "^7.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.17",
        "ramsey/uuid": "^4.7",
        "symfony/dotenv": "^7.0.0",
        "ramsey/uuid-doctrine": "^2.0",
        "kwn/php-rdkafka-stubs": "^2.2",
        "doctrine/common": "^3.4",
        "symfony/twig-bundle": "^7.0.0",
        "symfony/options-resolver": "^7.0.0",
        "symfony/asset": "^7.0.0",
        "twig/twig": "^3.7.1|^3.8.0",
        "twig/extra-bundle": "^3.7.1|^3.8.0",
        "symfony/validator": "^7.0.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.24",
        "symfony/property-access": "^7.0.0",
        "symfony/property-info": "^7.0.0",
        "symfony/serializer": "^7.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "symfony/polyfill": "^1.28",
        "vimeo/psalm": "^5.17",
        "phpunit/phpunit": "^10.5",
        "slevomat/coding-standard": "^8.14"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0.0"
        }
    }
}
